// Print numbers from 1 to n in Increasing Order?
public class Print_num_1ton {
    public static void printInc(int n){
        if(n == 1){
            System.out.print(n + " ");
            return;
        }
        printInc(n-1);
        System.out.print(n + " ");
    }
    public static void main(String args[]){
        int n = 5;
        printInc(n);
    }
}
